#include "main.h"
#include <stdio.h>

/**
 * main -  programme that prints the number of arguments passed into it
 * @argc: argument passed into main
 * @argv: argument passed into main
 * Return: Always 0'
 */
int main(int argc, char **argv __attribute__((unused)))
{
	if (argc > 0)
	{
		printf("%d\n", argc);
	}
	return (0);
}
